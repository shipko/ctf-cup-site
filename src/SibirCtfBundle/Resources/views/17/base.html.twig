<body>
<div class="brand">
<div class="container">
    <div class="row">
        <div class="col-sm-4">
            <a href="{{ path('seventeen_index') }}" style="text-decoration:none; color: #fff">
                <img src="{{ asset('img/logo.ai.png') }}" alt="" style="height: 130px; margin: 20px 0;">
            </a>
        </div>
        <div class="col-sm-5 col-sm-offset-2" style="float: right; height: 170px;padding-top: 30px">
            {% if app.user %}
            {% if is_granted('ROLE_ADMIN') %}<a href="{{ path('admin-index') }}"style="text-decoration: none;"><button type="button" class="btn btn-warning btn-lg">Админка</button></a>{% endif %}
                <a href="{{ path('cabinet') }}"style="text-decoration: none;"><button type="button" class="btn btn-info btn-lg">Кабинет</button></a>
            {% else %}
            <a href="{{ path('cabinet') }}"style="text-decoration: none;"><button type="button" class="btn btn-success btn-lg">Войти</button></a>
            <a href="{{ path('seventeen_signup_all') }}"style="text-decoration: none;"><button type="button" class="btn btn-success btn-lg">Принять участие</button></a>
            {% endif %}
        </div>
    </div>
</div>
</div>
<!-- Navigation -->
<nav class="navbar navbar-default" role="navigation">
    <div class="container">
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('seventeen_index') }}">SibirCTF 2017</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li><a href="{{ path('seventeen_rules') }}">Правила</a></li>
                {#<li><a href="{{ path('seventeen_results') }}">Результаты</a></li>#}
                <li><a href="{{ path('seventeen_participants') }}">Участники</a></li>
                <li><a href="{{ path('seventeen_timetable') }}">Расписание</a></li>
                <li><a href="{{ path('seventeen_teams') }}">Команда</a></li>
                <li><a href="{{ path('seventeen_sponsors') }}">Организаторы</a></li>
                {#<li><a href="{{ path('seventeen_news') }}">Новости</a></li>#}
                <li><a href="{{ path('seventeen_conference') }}">Конференция</a></li>
                {% if app.user %}<li><a href="{{ path('cabinet_logout') }}">Выйти</a></li>{% endif %}
            </ul>
        </div>
        <!-- /.navbar-collapse -->
    </div>
    <!-- /.container -->
</nav>

<div class="container">

    {% block timer %}{% endblock %}
    {% block body %}{% endblock %}
</div>
<!-- /.container -->

<footer>
    <div class="container">
        <div class="row">
            <div class="col-lg-10 text-center">
                <p>&copy; SibirCTF</p>

            </div>
            <div class="col-lg-2 text-center">
                <div style="margin-top: 50px;" id="vk_contact_us"></div>
            </div>
        </div>
    </div>
</footer>


